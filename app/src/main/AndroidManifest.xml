<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kylix.submissionbajp1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_lixflix_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_lixflix_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SubmissionBAJP1"
        android:fullBackupContent="@xml/backup_descriptor">
        <activity android:name=".ui.activities.SplashActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.activities.DetailActivity" />
        <activity android:name=".ui.activities.MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>